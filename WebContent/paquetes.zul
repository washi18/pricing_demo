					<label value="@bind(vmPaquete.oPaquete.cTituloIdioma1)" zclass="lbl_nombre_paquete" visible="@bind(vmPaquete.oPaquete.manejo_yourself)"></label>
					<space/>
					<label value="N° DIAS Y NOCHES:" zclass="lbl_subtitle_nombre_paquete"/>
					<label value="@bind(vmPaquete.oPaquete.dias_noches)" zclass="lbl_nombre_paquete"/>
				</div>
				<div zclass="div_contenedor_nuevo_paquete" visible="@bind(vmPaquete.select_manejo)">
					<div zclass="div_content_datos_paquete">
						<div id="div_contenedor_idioma" visible="false">
							<div zclass="div_contiene_banderas">
								<image src="img/Flag-of-Spain.png" sclass="estilo_banderas"
										onClick="txtSpainName.setVisible(true);txtInglesName.setVisible(false);
												txtPortuName.setVisible(false)"/>
								<image src="img/en.png" sclass="estilo_banderas"
										onClick="txtSpainName.setVisible(false);txtInglesName.setVisible(true);
												txtPortuName.setVisible(false)"/>
								<image src="img/Flag_of_Brazil.png" sclass="estilo_banderas"
										onClick="txtSpainName.setVisible(false);txtInglesName.setVisible(false);
												txtPortuName.setVisible(true)"/>
							</div>
<!-- 							<div> -->
<!-- 								<label value="COD PAQUETE:" zclass="lbl_llenar_datos"></label> -->
<!-- 								<textbox placeholder="Inserte codigo Paquete" value="@bind(vmPaquete.oPaquete.cPaqueteCod)" zclass="txt_llenar_datos" style="text-transform: uppercase;" width="100%"></textbox> -->
<!-- 							</div> -->
							<div>
								<label value="PAQUETE: " zclass="lbl_llenar_datos"/>
								<textbox value="@bind(vmPaquete.oPaquete.cTituloIdioma1)" zclass="txt_llenar_datos" id="txtSpainName" placeholder="ESPAÑOL" width="100%" visible="true"
										 onChange="@command('asignarNamePaquete')"/>
								<textbox value="@bind(vmPaquete.oPaquete.cTituloIdioma2)" zclass="txt_llenar_datos" id="txtInglesName" placeholder="INGLES" width="100%" visible="false"/>
								<textbox value="@bind(vmPaquete.oPaquete.cTituloIdioma3)" zclass="txt_llenar_datos" id="txtPortuName" placeholder="PORTUGUES" width="100%" visible="false"/>
							</div>
						</div>
						<div id="div_lbl_solo_camino_inca" visible="false">
							<separator bar="true"/>
							<label value="N° DE DIAS:" zclass="lbl_llenar_datos" />
							<intbox value="@bind(vmPaquete.oPaquete.nDias)" width="80px" placeholder="DIAS" sclass="txt_llenar_datos"
									onChange="@command('diasCaminoInka',dias=self.value)"/>
						</div>
						<div id="div_conDestino" visible="false">
							<radiogroup sclass="style_restricciones_conDestino" onCheck="@command('select_paquete_conDestino',opcion=self.selectedItem.value)">
								<radio label="CON DESTINO" checked="@bind(vmPaquete.oPaquete.conDestino)" value="con_destino" onCheck="div_select_destinos.setVisible(true);div_select_sindestinos.setVisible(false)"/>
								<radio label="SIN DESTINO" checked="@bind(vmPaquete.oPaquete.sinDestino)" value="sin_destino" onCheck="div_select_destinos.setVisible(false);div_select_sindestinos.setVisible(true)"/>
							</radiogroup>
							<div id="div_select_sindestinos" visible="true">
								<label value="N° DE DIAS:" zclass="lbl_llenar_datos" />
								<intbox value="@bind(vmPaquete.oPaquete.nDias)" width="80px" placeholder="DIAS" sclass="txt_llenar_datos"
										onChange="@command('diasSinDestino',dias=self.value)"/>
							</div>
						</div>
						<div id="div_manejo_yourself" visible="false">
							<div class="cmb_lugar">
								<label value="DESTINO:" zclass="lbl_llenar_datos"/>
								<combobox id="cbLugares" readonly="true" placeholder="Seleccionar destino" width="100%" onSelect="@command('seleccionardestinoInsertar',codDestino=self.selectedItem.label,manejoYourself=self.selectedItem.value)">
										<comboitem value="MACHUPICCHU" label="Machupicchu"></comboitem>
										<comboitem value="MACHUPICCHU_HUAYNAPICCHU_1G" label="Machupicchu + Huaynapicchu 1G 7:00-8:00 a.m."></comboitem>
										<comboitem value="MACHUPICCHU_HUAYNAPICCHU_2G" label="Machupicchu + Huaynapicchu 2G 10:00-11:00 a.m."></comboitem>
										<comboitem value="MACHUPICCHU_MONTANA_1G" label="Machupicchu + Montaña 7:00 a.m.-8:00 a.m."></comboitem>
										<comboitem value="MACHUPICCHU_MONTANA_2G" label="Machupicchu + Montaña 9:00 a.m.-10:00 a.m."></comboitem>
<!-- 										<comboitem value="CAMINO_INKA_CLASICO" label="Camino Inka 4 dias 3 noche 7:00 a.m.-10:00 a.m." ></comboitem> -->
<!-- 										<comboitem value="CAMINO_INKA_CORTO" label="Camino Inka 2 dias 1 noche 7:00 a.m.-10:00 a.m." ></comboitem> -->
								</combobox>
<!-- 								<div> -->
<!-- 									<separator bar="true" orient="horizontal"></separator> -->
<!-- 									<label value="NOMBRE DEL PAQUETE :" zclass="lbl_llenar_datos"/> -->
<!-- 									<textbox value="@bind(vmPaquete.oPaquete.cUrlReferenciaPaquete)" -->
<!-- 											placeholder="NOMBRE PAQUETE" zclass="txt_llenar_datos"/> -->
<!-- 									<separator bar="true" orient="horizontal"></separator> -->
<!-- 								</div> -->
							</div>
							<separator orient="horizontal"></separator>
							<label value="COSTO ($): " zclass="lbl_llenar_datos"/>
							<textbox value="@bind(vmPaquete.oPaquete.nPrecio1_text)" width="80px" placeholder="PRECIO" zclass="txt_llenar_datos"
											 onChange="@command('changePrecios_nuevo',precio=self.value,descuento='precio1',componente=self)"/>
							<space></space>
							<div>
								<separator bar="true" orient="horizontal"></separator>
								<label value="URL DE REFERENCIA:" zclass="lbl_llenar_datos"/>
								<textbox value="@bind(vmPaquete.oPaquete.cUrlReferenciaPaquete)"
										placeholder="URL" zclass="txt_llenar_datos"/>
							</div>
						</div>
						<div id ="div_caminoinka_manejopropio" visible="false">
							<separator bar="true"/>
							<radiogroup sclass="style_restricciones_conDestino" onCheck="@command('select_paquete_conDescuento',opcion=self.selectedItem.value)">
								<radio label="CON DESCUENTO" checked="@bind(vmPaquete.oPaquete.conDescuento)" value="con_descuento" onCheck="div_conDescuento.setVisible(true);div_sinDescuento.setVisible(false)"/>
								<radio label="SIN DESCUENTO" checked="@bind(vmPaquete.oPaquete.sinDescuento)" value="sin_descuento" onCheck="div_conDescuento.setVisible(false);div_sinDescuento.setVisible(true)"/>
							</radiogroup>
							<div>
								<separator bar="true" orient="horizontal"></separator>
								<label value="URL DE REFERENCIA:" zclass="lbl_llenar_datos"/>
								<textbox value="@bind(vmPaquete.oPaquete.cUrlReferenciaPaquete)"
										placeholder="URL" zclass="txt_llenar_datos"/>
							</div>
							<div id="div_sinDescuento" visible="true">
								<div>
									<separator bar="true"/>
									<label value="COSTO ($): " zclass="lbl_llenar_datos"/>
									<textbox value="@bind(vmPaquete.oPaquete.nPrecio1_text)" width="80px" placeholder="PRECIO" zclass="txt_llenar_datos"
											 onChange="@command('changePrecios_nuevo',precio=self.value,descuento='precio1',componente=self)"/>
									<space></space>
								</div>
							</div>
							<div id="div_conDescuento" visible="false">
								<div>
									<separator bar="true"/>
									<label value="COSTO 1($): " zclass="lbl_llenar_datos"/>
									<textbox value="@bind(vmPaquete.oPaquete.nPrecio1_text)" width="80px" placeholder="PRECIO" zclass="txt_llenar_datos"
											 onChange="@command('changePrecios_nuevo',precio=self.value,descuento='precio1',componente=self)"/>
									<space></space>
								</div>
								<div>
									<separator bar="true"/>
									<label value="COSTO 2($): " zclass="lbl_llenar_datos"/>
									<textbox value="@bind(vmPaquete.oPaquete.nPrecio2_text)" width="80px" placeholder="PRECIO" zclass="txt_llenar_datos"
											 onChange="@command('changePrecios_nuevo',precio=self.value,descuento='precio2',componente=self)"/>
									<space></space>
								</div>
								<div>
									<separator bar="true"/>
									<label value="COSTO 3($): " zclass="lbl_llenar_datos"/>
									<textbox value="@bind(vmPaquete.oPaquete.nPrecio3_text)" width="80px" placeholder="PRECIO" zclass="txt_llenar_datos"
											 onChange="@command('changePrecios_nuevo',precio=self.value,descuento='precio3',componente=self)"/>
									<space></space>
								</div>
								<div>
									<separator bar="true"/>
									<label value="COSTO 4($): " zclass="lbl_llenar_datos"/>
									<textbox value="@bind(vmPaquete.oPaquete.nPrecio4_text)" width="80px" placeholder="PRECIO" zclass="txt_llenar_datos"
											 onChange="@command('changePrecios_nuevo',precio=self.value,descuento='precio4',componente=self)"/>
									<space></space>
								</div>
								<div>
									<separator bar="true"/>
									<label value="COSTO 5($): " zclass="lbl_llenar_datos"/>
									<textbox value="@bind(vmPaquete.oPaquete.nPrecio5_text)" width="80px" placeholder="PRECIO" zclass="txt_llenar_datos"
											 onChange="@command('changePrecios_nuevo',precio=self.value,descuento='precio5',componente=self)"/>
									<space></space>
								</div>
							</div>
						</div>
						<div zclass="div_space_entre_botones">
								<button label="Configurar Paquete" onClick="@command('abrirConfigurarPaquete') @global-command('obtenerDatosConfigPaquete',paquete=vmPaquete.oPaquete)" zclass="btn_guardar"/>
						</div>
						<div zclass="div_space_entre_botones">
								<button label="Editar Itinerario" onClick="@command('abrirEditorItinerario',cPaquete=vmPaquete.oPaquete)" zclass="btn_guardar"/>
						</div>
						<div zclass="div_space_entre_botones">
								<button label="Editar Descripcion" onClick="@command('abrirEditorDescripcion',cPaquete=vmPaquete.oPaquete)" zclass="btn_guardar"/>
						</div>
						<div zclass="div_space_entre_botones">
								<button label="Asignar imagenes existentes" 
										onClick="@command('invocaImagenesExistentes',paquete=vmPaquete.oPaquete,opcion=1)"
										zclass="btn_guardar"/>
						</div>
						<div zclass="div_space_entre_botones">
								<button label="Subir Imagenes del paquete" 
										onClick="@command('uploadImagenes',cPaquete=vmPaquete.oPaquete,componente=self)"
										zclass="btn_guardar"/>
						</div>
						<div zclass="div_space_entre_botones">
								<button label="Ver imagenes subidas" 
										onClick="@command('invocaImagenesSubidas') @global-command('muestraImagenesSubidasPaquete',cPaquete=vmPaquete.oPaquete)"
										zclass="btn_guardar"/>
						</div>
						<button label="Guardar" zclass="btn_guardar" onClick="@command('insertarPaquete',componente=self)"/>
						<window mode="modal" sclass="win_descHotel" visible="@load(vmPaquete.oPaquete.abrirEditorItinerario)" position="center">
								<div zclass="div_cerrar_descHotel">
									<button label="X" onClick="@command('cerrarEditorItinerario',cPaquete=vmPaquete.oPaquete)" zclass="btn_cerrar_descHotel"/>
								</div>
								<div zclass="div_content_datos">
									<div>
										<div zclass="div_contiene_banderas">
											<image src="img/Flag-of-Spain.png" sclass="estilo_banderas"
													onClick="txtSpainItinerario.setVisible(true);
															txtInglesItinerario.setVisible(false);txtPortuItinerario.setVisible(false)"/>
											<image src="img/en.png" sclass="estilo_banderas"
													onClick="txtSpainItinerario.setVisible(false);
															txtInglesItinerario.setVisible(true);txtPortuItinerario.setVisible(false)"/>
											<image src="img/Flag_of_Brazil.png" sclass="estilo_banderas"
													onClick="txtSpainItinerario.setVisible(false);
															txtInglesItinerario.setVisible(false);txtPortuItinerario.setVisible(true)"/>
										</div>
										<div>
											<label value="ITINERARIO: " zclass="lbl_llenar_datos"/>
											<ckeditor value="@bind(vmPaquete.oPaquete.cItinerarioIdioma1)" id="txtSpainItinerario" visible="true" width="100%"/>
											<ckeditor value="@bind(vmPaquete.oPaquete.cItinerarioIdioma2)" id="txtInglesItinerario" visible="false" width="100%"/>
											<ckeditor value="@bind(vmPaquete.oPaquete.cItinerarioIdioma3)" id="txtPortuItinerario" visible="false" width="100%"/>
										</div>
									</div>
								</div>
							</window>
							<window mode="modal" sclass="win_descHotel" visible="@load(vmPaquete.oPaquete.abrirEditorDescripcion)" position="center">
								<div zclass="div_cerrar_descHotel">
									<button label="X" onClick="@command('cerrarEditorDescripcion',cPaquete=vmPaquete.oPaquete)" zclass="btn_cerrar_descHotel"/>
								</div>
								<div zclass="div_content_datos">
									<div>
										<div zclass="div_contiene_banderas">
											<image src="img/Flag-of-Spain.png" sclass="estilo_banderas"
													onClick="txtSpainItinerario.setVisible(true);
															txtInglesItinerario.setVisible(false);txtPortuItinerario.setVisible(false)"/>
											<image src="img/en.png" sclass="estilo_banderas"
													onClick="txtSpainItinerario.setVisible(false);
															txtInglesItinerario.setVisible(true);txtPortuItinerario.setVisible(false)"/>
											<image src="img/Flag_of_Brazil.png" sclass="estilo_banderas"
													onClick="txtSpainItinerario.setVisible(false);
															txtInglesItinerario.setVisible(false);txtPortuItinerario.setVisible(true)"/>
										</div>
										<div>
											<label value="DESCRIPCION: " zclass="lbl_llenar_datos"/>
											<ckeditor value="@bind(vmPaquete.oPaquete.cDescripcionIdioma1)" id="txtSpainItinerario" visible="true" width="100%"/>
											<ckeditor value="@bind(vmPaquete.oPaquete.cDescripcionIdioma2)" id="txtInglesItinerario" visible="false" width="100%"/>
											<ckeditor value="@bind(vmPaquete.oPaquete.cDescripcionIdioma3)" id="txtPortuItinerario" visible="false" width="100%"/>
										</div>
									</div>
								</div>
							</window>
							<window mode="modal" sclass="win_imagenesHotel" visible="@load(vmPaquete.mostrarImagenesExistentes)" position="center">
								<div zclass="div_cerrar_descHotel">
									<label value="Escogio una imagen" 
											visible="@bind(vmPaquete.mostrarTextImgSeleccionado)"
											style="color:#3BB710;font-size:15px;"/>
									<label value="✔" zclass="lbl_ok" visible="@bind(vmPaquete.mostrarTextImgSeleccionado)"/>
									<label value="No escogio ninguna imagen" 
											visible="@bind(!vmPaquete.mostrarTextImgSeleccionado)"
											style="color:red;font-size:15px;"/>
									<label value="X" zclass="lbl_X" visible="@bind(!vmPaquete.mostrarTextImgSeleccionado)"/>
									<space></space>
									<button label="X" onClick="@command('cerrarImagenesExistentes')" zclass="btn_cerrar_descHotel"/>
								</div>
								<div sclass="div_contenido_descHotel">
											<div width="100%">
												<label value="Imagen:" sclass="lbl_llenar_datos"/>
												<textbox id="txtNameImage1" width="180px" placeholder="nombre imagen" sclass="txt_llenar_datos" onOK="@command('buscarImagen',texto=self.value)"/>
	<!-- 											<div class="centrar_boton_busqueda"> -->
													<button iconSclass="fa fa-search" height="20px" width="30px" zclass="estilo_btn_buscar" onClick="@command('buscarImagen',texto=txtNameImage1.value)"/>
	<!-- 											</div> -->
											</div>
											<space/>
											<div width="100%">
												<label value="Mostrar:" sclass="lbl_llenar_datos"></label>
												<combobox value="TODOS" onSelect="@command('selectTipoImagenExistente',tipo=self.selectedItem.value)">
													<comboitem label="TODOS" value="todos"/>
													<comboitem label="HOTELES" value="hoteles"/>
													<comboitem label="TOURS" value="tours"/>
													<comboitem label="SERVICIOS" value="servicios"/>
													<comboitem label="DESTINOS" value="destinos"/>
													<comboitem label="ANDROID" value="android"/>
												</combobox>
											</div>
								</div>
								<grid model="@bind(vmPaquete.lista4ImagenesExistentes)">
									<columns>
										<column label=""/>
									</columns>
									<template name="model">
										<row sclass="row_grid">
											<div zclass="div_content_columnas">
												<div zclass="div_content_col2">
													<div zclass="@load(each.galeria1.style_Select)"
														visible="@load(each.galeria1.visible)"
														onClick="@command('selectImagenExist',galeria4=each,galeria=each.galeria1,paquete=vmPaquete.oPaquete)">
														<image src="@bind(each.galeria1.rutaImagen)" width="100%" height="150px"/>
													</div>
													<div zclass="@load(each.galeria2.style_Select)"
														visible="@load(each.galeria2.visible)"
														onClick="@command('selectImagenExist',galeria4=each,galeria=each.galeria2,paquete=vmPaquete.oPaquete)">
														<image src="@bind(each.galeria2.rutaImagen)" width="100%" height="150px"/>
													</div>
												</div>
												<div zclass="div_content_col2">
													<div zclass="@load(each.galeria3.style_Select)"
														visible="@load(each.galeria3.visible)"
														onClick="@command('selectImagenExist',galeria4=each,galeria=each.galeria3,paquete=vmPaquete.oPaquete)">
														<image src="@bind(each.galeria3.rutaImagen)" width="100%" height="150px"/>
													</div>
													<div zclass="@load(each.galeria4.style_Select)"
														visible="@load(each.galeria4.visible)"
														onClick="@command('selectImagenExist',galeria4=each,galeria=each.galeria4,paquete=vmPaquete.oPaquete)">
														<image src="@bind(each.galeria4.rutaImagen)" width="100%" height="150px"/>
													</div>
												</div>
											</div>
										</row>
									</template>
								</grid>
							</window>
					</div>
