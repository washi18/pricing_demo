						<radio visible="@bind(vmPaquete.visibilidadYourself)" label="MANEJO YOURSELF" value="MANEJO_YOURSELF"
							   sclass="radio_yourself"
							   checked="@bind(vmPaquete.oPaquete.manejo_yourself)" 
							   onCheck="div_manejo_yourself.setVisible(true);
							   div_contenedor_idioma.setVisible(true);
							   div_lbl_solo_camino_inca.setVisible(false);
							   div_select_destinos.setVisible(false);
							   div_conDestino.setVisible(false);
							   div_caminoinka_manejopropio.setVisible(false)"/>
						<radio label="MANEJO SÓLO CAMINO INKA CLÁSICO" value="CAMINO_INKA_CLASICO"
							   sclass="radio_camino_inka"
							   checked="@bind(vmPaquete.oPaquete.manejo_camino_inca)" 
							   onCheck="div_manejo_yourself.setVisible(false);
							   div_caminoinka_manejopropio.setVisible(true);
							   div_contenedor_idioma.setVisible(true);
							   div_lbl_solo_camino_inca.setVisible(true);
							   div_select_destinos.setVisible(false);
							   div_conDestino.setVisible(false)"/>
<!-- 						<radio label="MANEJO DE DISPONIBILIDAD PROPIA" value="MANEJO_PROPIO" -->
<!-- 							   sclass="radio_manejo_propio" -->
<!-- 							   onCheck="div_lbl_solo_camino_inca.setVisible(false); -->
<!-- 							   div_conDestino.setVisible(true)"/> -->
						<radio label="MANEJO POR CALENDARIO NORMAL" value="MANEJO_NORMAL" 
							   sclass="radio_manejo_normal"
							   checked="@bind(vmPaquete.oPaquete.manejo_normal)"
							   onCheck="div_manejo_yourself.setVisible(false);
							   div_caminoinka_manejopropio.setVisible(true);
							   div_lbl_solo_camino_inca.setVisible(false);
							   div_contenedor_idioma.setVisible(true);
							   div_conDestino.setVisible(true)"/>
					</radiogroup>
				</div>
				<div visible="@bind(vmPaquete.select_manejo)">
					<separator spacing="5px"/>
					<label value="NOMBRE PAQUETE:" zclass="lbl_subtitle_nombre_paquete"/>
