																													<div width="100%" zclass="div_list_cell_menu">
																														<listbox>
																															<listhead sclass="listhead_datoGeneral">
																																<listheader label="@load(datoGeneral.cTituloIdioma1)" 
																																	onClick="@command('mostrarDatoGeneral',datoGeneral=datoGeneral)">
																																	<image src="@load(datoGeneral.cImagen)" height="40px" width="40px" style="float:right;"/>
																																</listheader>
																															</listhead>
																														</listbox>
																													</div>
																												</listcell>
																											</listitem>
																										</template>
																									</listbox>
																								</div>					
																							</listcell>
																						</listitem>
																					</template>
																				</listbox>
																			</div>
																		</listcell>
																	</listitem>
																</template>
															</listbox>
														</div>
													</listcell>
												</listitem>
											</template>
										</listbox>
