																					<listhead sclass="listhead_item">
																						<listheader label="@load(item.cTituloIdioma1)" 
																							onClick="@command('mostrarContentItems',item=item)">
																							<image src="@load(item.cImagen)" height="40px" width="40px" style="float:right;"/>
																						</listheader>
																					</listhead>
																					<template name="model" var="elemento">
																						<listitem visible="@bind(item.visibleContent)">
																							<listcell>
																								<div width="100%" zclass="div_list_cell_menu">
																									<listbox sclass="listados_menu" model="@load(elemento.listaDestinosMovil)">
																										<listhead sclass="listhead_elemento">
																											<listheader label="@load(elemento.cNombre1Idioma1)" 
																												onClick="@command('mostrarElemento_item',elemento=elemento)">
																												<image src="@load(elemento.cImagen1)" height="40px" width="40px" style="float:right;"/>
																											</listheader>
																										</listhead>
																										<template name="model" var="destinoMovil">
																											<listitem>
																												<listcell>
																													<div width="100%" zclass="div_list_cell_menu">
																														<listbox>
																															<listhead sclass="listhead_destino">
																																<listheader label="@load(destinoMovil.cDestino)" onClick="@command('mostrarDestino',destino=destinoMovil)"/>
																															</listhead>
																														</listbox>
																													</div>
																												</listcell>
																											</listitem>
																										</template>
																									</listbox>
																									<listbox sclass="listados_menu" model="@load(elemento.listaDatosGenerales)">
